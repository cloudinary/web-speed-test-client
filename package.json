{
  "name": "web-speed-test",
  "description": "web-speed-test Image Performance Analysis Client App",
  "version": "1.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudinaryLtd/web-speed-test-client.git"
  },
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "eslint --ext .jsx,.js --fix ./src",
    "upload:images": "node ./src/services/imageSync/cloudinaryImageSync.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "trailingComma": "none"
        }
      ]
    },
    "ignorePatterns": [
      "*.min.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "cloudinary-core": "^2.13.0",
    "cloudinary-react": "^1.8.1",
    "i18next": "^23.2.1",
    "numbro": "^2.3.6",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^5.3.4",
    "react-share": "^4.4.1",
    "react-tabs": "^3.2.3",
    "vite": "^5.0.12",
    "vite-plugin-minify": "^1.5.2",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "async": "^3.2.5",
    "cloudinary": "^1.41.3",
    "dotenv": "^16.3.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.70.0"
  }
}
